<div class="col-md-6 col-md-offset-3">
  <% if params[:plan] == '2' %>
    <h2 class="text-center">Pro Account</h2>
  <% else %>
    <h2 class="text-center"> Basic Account</h2>
  <% end %>  
</div>
<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <h2>Sign up</h2>
    <% if params[:plan] == '2' %>
      <%= render "pro_form" %>
    <% else %>
      <%= render "basic_form" %>
    <% end %>
    <%= render "devise/shared/links" %>
  </div>
</div>